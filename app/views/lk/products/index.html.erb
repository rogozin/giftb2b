<% page_title "Список товаров" -%>
<h1>Список товаров</h1>
<div class="spacer"></div>
<div><%= link_to 'Добавить новый товар', new_lk_product_path  %></div>
<div class="spacer"></div>
<% if @products.present? -%>
<%= render :partial => "products", :locals => {:products => @products, :should_check => false} %>
<% else -%>
<p>Товары отсутствуют</p>
<% end -%>


