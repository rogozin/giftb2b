<ul>
<% property.property_values.each do |item| -%>
  <li><%= check_box_tag defined?(check_box_name) ? check_box_name : "property_values[]", item.id, selected && selected.compact.map(&:to_i).include?(item.id), :id => "#{item.id}" %>
  <%= label_tag "#{item.id}",  item.value %>
  </li>  
<% end -%>
</ul>
