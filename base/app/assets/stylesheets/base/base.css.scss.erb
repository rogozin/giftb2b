@mixin red_gradient {
  background: -webkit-linear-gradient(#cc6666, #a91e4c);
  background: -moz-linear-gradient(#cc6666, #a91e4c);
  background: -ms-linear-gradient(#cc6666, #a91e4c);
  background: -o-linear-gradient(#cc6666, #a91e4c);
  background: linear-gradient(#cc6666, #a91e4c);
  -pie-background: linear-gradient(#cc6666, #a91e4c);
}

@mixin red_gradient_hover {
  background: -webkit-linear-gradient(#cc6666, #BD4656);
  background: -moz-linear-gradient(#cc6666, #BD4656);
  background: -ms-linear-gradient(#cc6666, #BD4656);
  background: -o-linear-gradient(#cc6666, #BD4656);
  background: linear-gradient(#cc6666, #BD4656);
  -pie-background: linear-gradient(#cc6666, #BD4656);
} 
 
input[type="submit"], input[type="button"], .r_button {
  color:#e8eef4;
  border: 1px solid #773841;
  background: #a91e4c;
  @include red_gradient;
  &:hover  {
    @include red_gradient_hover;
  }
}


.ui-dialog .ui-dialog-content {
